/ Start: Topbar
%header#topbar.ph10
  .topbar-left
    %ul.nav.nav-list.nav-list-topbar.pull-left
      %li.active
        =link_to 'Dashboard', admin_path
      %li
        =link_to 'Проекты', admin_projects_path
      %li
        =link_to 'Услуги', admin_services_path
      %li
        =link_to 'Сотрудники', admin_teammembers_path
  .topbar-right.hidden-xs.hidden-sm
    =link_to new_admin_project_path, class: 'btn btn-default btn-sm fw600 ml10' do
      %span.fa.fa-plus.pr5
      Новый проект
    =link_to new_admin_service_path, class: 'btn btn-default btn-sm fw600 ml10' do
      %span.fa.fa-plus.pr5
      Новая услуга
    =link_to new_admin_teammember_path, class: 'btn btn-default btn-sm fw600 ml10' do
      %span.fa.fa-user.pr5
      Новый сотрудник
  / End: Topbar